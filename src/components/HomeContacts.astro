---
import informationsDatas from "../datas/informationDatas.json"
import Contact from "../components/Contact.vue"
---

<div class=" contact-header flex flex-col justify-center items-center bg-persoBlue w-20 h-32 space-y-3 mr-2 rounded-md shadow-xl">
    <a href={informationsDatas.instaUrl} target="_blank"><img src={Astro.resolve('/assets/instagram-icon.svg')}
            alt="instagram" class=" w-11 h-11 text-lightColor mb-3"></a>
    <Contact client:visible />
</div>